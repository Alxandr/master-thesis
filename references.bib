title = {{Making Examinations more Valid, Meaningful and Motivating: The Online Exams Service at ETH Zurich}},
url = {http://www.eunis.org/erai/2014-1/{\%}5Cnhttp://www.eunis.org/eunis2014/papers/{\%}5Cnhttp://www.eunis.org/download/2014/papers/eunis2014{\_}submission{\_}69.pdf},
