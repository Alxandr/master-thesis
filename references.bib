@article{Harpp1993,
abstract = {Computer programs are being used to combat cheating in the classroom. The high levels of cheating among students have come about largely because of permissive seating arrangements in overcrowded rooms, and the use of a single exam paper. Software programs have been developed which can identify cheats. One operates by flagging those individuals with errors in common. The other assesses the number of correct answers against the number of differences in the exam paper for a suspect pair. The accumulated evidence indicates that significant amounts of extreme cheating and partial copying are going on. Both types of cheating can be positively identified. Despite their intrusive nature, simple measures such as seat and examination scrambling are recommended in order to eliminate the problem of cheating in multiple-choice examinations.},
author = {Harpp, David N. and Hogan, James J.},
